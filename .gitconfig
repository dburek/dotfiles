[difftool "sourcetree"]
	cmd = "'' "
[mergetool "sourcetree"]
	cmd = "'' "
	trustExitCode = true
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	logo = log --oneline
	logog = log --oneline --graph
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset'
	cia = commit --allow-empty
[diff]
	tool = winmerge
[difftool]
	prompt = false
